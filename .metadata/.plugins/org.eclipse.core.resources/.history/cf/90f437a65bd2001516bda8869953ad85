
public class tes {
	

	public void mutuje() {
		// UstawPrawdMutacji();
		int locus = 0;
		String czesc1 = "0";
		String czesc2 = "0";
		String czesc3 = "0";
		//zadanie1 obiektZadanie = zadanie1.zwrocObiekt();
		int a = 1;
		int b = 2;
		if (a < b) {
			locus = 2;

			czesc1 = "01234".substring(0, locus);
			czesc2 = "01234".substring(locus, locus);
			czesc3 = "01234".substring(locus + 1, 5);
			System.out.println("stara czesc 2 "+czesc2);
			if (czesc2 == "2") {
				czesc2 = "1";
			} else {
				czesc2 = "0";
			}
			String wynik = czesc1 + czesc2 + czesc3;
			System.out.println("cz1  "+czesc1+"\ncz2  "+czesc2+"\ncz3  "+czesc3+
					"\nWynik :" + wynik);
		}

	

}
public static void main(String Args[]){
	tes test = new tes();
	test.mutuje();
}
}